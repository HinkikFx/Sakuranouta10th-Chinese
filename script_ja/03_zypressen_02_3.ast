},
},
},
linkback="block_00082",
linknext="block_00084",
},
block_00084={
{"text"},
text={
vo={
{"vo",file="fem_rna_001371",ch="rna"},
},
ja={
{
name={"里奈"},
"「気が合いますね。仲良くしましょう」",
{"rt2"},
},
},
},
linkback="block_00083",
linknext="block_00085",
},
block_00085={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1i0000",face="i0009",head="yum_z1i",x=0,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_001243",ch="yum"},
},
ja={
{
name={"優美"},
