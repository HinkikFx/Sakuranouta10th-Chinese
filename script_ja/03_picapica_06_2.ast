vo={
{"vo",file="fem_mkd_002916",ch="mkd"},
},
ja={
{
name={"真琴"},
"「何よ、それ……今更。……そうだけど」",
{"rt2"},
},
},
},
linkback="block_00096",
linknext="block_00098",
},
block_00098={
{"text"},
