{"bg",file="ev_uni_c53_07_yuri",id=7,lv=7,y=0,speed=3000,path=":cg/cg_ev_uni_c53/",set="cg_ev_uni_c53",y2=1100,sync=0},
{"bg",file="ev_uni_c53_03_moon",id=3,lv=3,y=0,speed=3000,path=":cg/cg_ev_uni_c53/",set="cg_ev_uni_c53",y2=-600,sync=0},
{"bg",file="ev_uni_c53_04_star",id=4,lv=4,y=0,speed=3000,path=":cg/cg_ev_uni_c53/",set="cg_ev_uni_c53",y2=-600,sync=0},
{"extrans",time=3000,sync=1},
