{"fg",ch="香奈",size="z1",mode=3,path=":fg/kan/z1/",file="kan_z1a0100",face="a0001",head="kan_z1a",lv=4,id=9,sync=0},
{"extrans",time=500},
{"text"},
text={
vo={
{"vo",file="fem_knd_000123",ch="knd"},
},
ja={
{
name={"香奈"},
"「……」",
{"rt2"},
},
},
},
linkback="block_00130",
linknext="block_00132",
},
block_00132={
{"fg",ch="香奈",size="z1",mode=3,path=":fg/kan/z1/",file="kan_z1a0100",face="a0006",head="kan_z1a",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_knd_000124",ch="knd"},
},
ja={
{
name={"香奈"},
"「この火事って……」",
{"rt2"},
},
},
},
linkback="block_00131",
linknext="block_00133",
},
block_00133={
{"text"},
text={
ja={
{
"私はその記事に釘付けとなった。",
{"rt2"},
},
},
},
linkback="block_00132",
linknext="block_00134",
},
block_00134={
{"text"},
text={
ja={
{
"真実を捕まえたのだから。",
{"rt2"},
},
},
},
linkback="block_00133",
linknext="block_00135",
},
block_00135={
{"bgm",stop=1,time=1500},
