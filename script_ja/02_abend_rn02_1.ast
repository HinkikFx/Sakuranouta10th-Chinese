linknext="block_00115",
},
block_00115={
{"fg",ch="稟",size="z1",mode=3,path=":fg/rin/z1/",file="rin_z1a0300",face="a0014j",head="rin_z1a",y=10,lv=4,id=9,zoom=110},
{"fgact",ch="稟",act="いいえ",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_rnd_000552",ch="rnd"},
},
ja={
{
name={"稟"},
"「あ、いや、ち、違っ、違います。そんなことないですよ！」",
{"rt2"},
},
},
},
linkback="block_00114",
