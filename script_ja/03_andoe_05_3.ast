ja={
{
name={"直哉"},
"「歩くって？」",
{"rt2"},
},
},
},
linkback="block_00079",
linknext="block_00081",
},
block_00081={
{"fg",ch="吹",size="z1",mode=3,path=":fg/sui/z1/",file="sui_z1a0000",face="a0002",head="sui_z1a",x=-345,lv=4,id=10},
