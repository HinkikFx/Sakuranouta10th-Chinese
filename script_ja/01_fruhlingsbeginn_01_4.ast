ja={
{
name={"真琴"},
"「当たり前じゃない一口だけよ！　……ぱく……もぐ……もぐ……」",
{"rt2"},
},
},
},
linkback="block_00218",
linknext="block_00220",
},
block_00220={
{"text"},
text={
ja={
{
name={"直哉"},
"「どうだ……」",
{"rt2"},
},
