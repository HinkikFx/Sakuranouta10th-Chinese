vo={
{"vo",file="fem_rnd_003168",ch="rnd"},
},
ja={
{
name={"稟"},
"「あっ」",
{"rt2"},
},
},
},
linkback="block_00107",
linknext="block_00109",
},
block_00109={
{"text"},
text={
ja={
{
"私は思い出した。",
{"rt2"},
},
},
},
