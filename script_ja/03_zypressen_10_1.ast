},
block_00243={
{"fg",ch="里奈",size="z1",mode=3,path=":fg/rna/z1/",file="rna_z1d0000",face="d0003",head="rna_z1d",x=-345,lv=4,id=5},
{"text"},
text={
vo={
{"vo",file="fem_rna_002191",ch="rna"},
},
