{"bg",file="zev_unq_c03_02",id=3,lv=3,zoom=51,alpha=255,time=1000,path=":ev/ev_unq_c03/",z2=50,set="ev_unq_c03",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
{"bg",file="zev_unq_c02_02",id=4,lv=4,zoom=53,alpha=100,time=1200,path=":ev/ev_unq_c02/",z2=50,set="ev_unq_c02",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
{"bg",file="zev_unq_c03_02",id=5,lv=5,zoom=53,alpha=100,time=1200,path=":ev/ev_unq_c03/",z2=50,set="ev_unq_c03",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
{"bg",file="zev_unq_c02_02",id=6,lv=6,zoom=55,alpha=64,time=1400,path=":ev/ev_unq_c02/",z2=50,set="ev_unq_c02",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
{"bg",file="zev_unq_c03_02",id=7,lv=7,zoom=55,alpha=64,time=1400,path=":ev/ev_unq_c03/",z2=50,set="ev_unq_c03",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
{"bg",file="zev_unq_c02_02",id=8,lv=8,zoom=60,alpha=32,time=1600,path=":ev/ev_unq_c02/",z2=50,set="ev_unq_c02",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
{"bg",file="zev_unq_c03_02",id=9,lv=9,zoom=60,alpha=32,time=1600,path=":ev/ev_unq_c03/",z2=50,set="ev_unq_c03",ax=1920,ay=1080,bx=-960,by=-540,sync=0},
