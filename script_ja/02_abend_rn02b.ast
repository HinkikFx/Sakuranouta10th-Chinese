},
block_00127={
{"fg",ch="雫",size="z1",mode=3,path=":fg/shz/z1/",file="shz_z1a0300",face="a0008",head="shz_z1a",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_szd_000191",ch="szd"},
},
ja={
{
name={"雫"},
"「つまり水菜さんの事を……」",
{"rt2"},
},
},
},
linkback="block_00126",
linknext="block_00128",
},
block_00128={
{"text"},
text={
ja={
{
name={"直哉"},
"「違うわ」",
{"rt2"},
},
},
},
linkback="block_00127",
linknext="block_00129",
},
block_00129={
{"fg",ch="雫",size="z1",mode=3,path=":fg/shz/z1/",file="shz_z1a0300",face="a0012",head="shz_z1a",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_szd_000192",ch="szd"},
},
ja={
{
name={"雫"},
"「違うのですか？」",
{"rt2"},
},
},
},
linkback="block_00128",
linknext="block_00130",
},
block_00130={
{"text"},
text={
ja={
{
name={"直哉"},
"「オランピアの事が出てきたからといって、マネの代表作と母親を必ず重ねて考えるわけではないだろ」",
{"rt2"},
},
},
},
linkback="block_00129",
linknext="block_00131",
},
block_00131={
