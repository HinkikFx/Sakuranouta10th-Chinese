},
block_00094={
{"fg",ch="ルリヲ",size="z1",mode=3,path=":fg/rur/z1/",file="rur_z1c0200",face="c0002",head="rur_z1c",x=100,lv=4,id=10},
{"fgact",ch="ルリヲ",act="いいえ",lv=4,id=10},
{"text"},
text={
