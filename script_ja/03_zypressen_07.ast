vo={
{"vo",file="fem_rna_001981",ch="rna"},
},
ja={
{
name={"里奈"},
"「せ、先輩って卑怯ですよ」",
{"rt2"},
},
},
},
linkback="block_00212",
linknext="block_00214",
},
block_00214={
{"text"},
text={
ja={
{
name={"直哉"},
"「何が？」",
{"rt2"},
},
},
},
linkback="block_00213",
