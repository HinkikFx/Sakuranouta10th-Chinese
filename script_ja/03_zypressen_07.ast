{"fg",ch="里奈",size="z1",mode=3,path=":fg/rna/z1/",file="rna_z1d0100",face="d0010j",head="rna_z1d",lv=4,id=5},
{"fgact",ch="里奈",act="おじぎ",lv=4,id=5},
{"text"},
