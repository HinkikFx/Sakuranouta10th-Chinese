},
block_00215={
{"fg",ch="里奈",size="z1",mode=3,path=":fg/rna/z1/",file="rna_z1d0100",face="d0009i",head="rna_z1d",lv=4,id=5},
{"text"},
text={
vo={
{"vo",file="fem_rna_001982",ch="rna"},
},
ja={
{
name={"里奈"},
