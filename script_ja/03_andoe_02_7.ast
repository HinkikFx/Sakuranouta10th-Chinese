},
block_00361={
{"fg",ch="フリッドマン",size="z1",mode=3,path=":fg/fri/z1/",file="fri_z1b0000",face="b0011",head="fri_z1b",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="man_frd_000022",ch="frd"},
},
ja={
{
name={"フリッドマン"},
"「追いかけ追いつきいなくなる」",
{"rt2"},
},
},
},
linkback="block_00360",
