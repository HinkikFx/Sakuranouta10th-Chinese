text={
vo={
{"vo",file="fem_jkz_000002",ch="jkz"},
},
ja={
{
name={"女子校生Z","女子校生Ｃ"},
"「藍ちゃんそれってもしかして」",
{"rt2"},
},
},
},
linkback="block_00061",
linknext="block_00063",
},
block_00063={
