delay={
vl01={
{"fg",ch="吹",size="z1",mode=3,path=":fg/sui/z1/",file="sui_z1a0100",face="a0017",head="sui_z1a",lv=4,id=5},
{"fgact",ch="吹",act="うなづく",lv=4,id=5},
},
},
text={
vo={
{"vo",file="fem_sud_000719",ch="sud"},
},
ja={
{
name={"吹"},
"「意味分かりません！　ったく、とりあえず吹は行きますからね！」",
{"rt2"},
},
},
},
linkback="block_00173",
linknext="block_00175",
},
block_00175={
{"text"},
text={
ja={
{
name={"直哉"},
"「ああ、なんか良く分からんが、いってらっしゃいだ」",
{"rt2"},
},
},
},
linkback="block_00174",
linknext="block_00176",
