vo={
{"vo",file="man_yod_000067",ch="yod"},
},
ja={
{
name={"義貞"},
"「どうしましたか？」",
{"rt2"},
},
},
},
linkback="block_00091",
linknext="block_00093",
},
block_00093={
{"mwact",act="びっくり",size=5},
{"text"},
text={
vo={
{"vo",file="fem_had_000033",ch="had"},
