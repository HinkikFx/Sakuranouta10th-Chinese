text={
vo={
{"vo",file="man_yod_000069",ch="yod"},
},
ja={
{
name={"義貞"},
"「たしかに顔が真っ赤です」",
{"rt2"},
},
},
},
linkback="block_00103",
linknext="block_00105",
},
