vo={
{"vo",file="man_yod_000048",ch="yod"},
},
ja={
{
name={"義貞"},
"「名は無い。ただの枯れ木の丘」",
{"rt2"},
},
},
},
linkback="block_00038",
linknext="block_00040",
},
block_00040={
