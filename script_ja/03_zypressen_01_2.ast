vo={
{"vo",file="man_yod_000068",ch="yod"},
},
ja={
{
name={"義貞"},
"「体調が悪い？　それはいけないっ」",
{"rt2"},
},
},
},
linkback="block_00098",
linknext="block_00100",
},
