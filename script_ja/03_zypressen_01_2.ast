vo={
{"vo",file="man_yod_000050",ch="yod"},
},
ja={
{
name={"義貞"},
"「いや。そんなことより先に礼を申し上げるべきでしたな」",
{"rt2"},
},
},
},
linkback="block_00040",
linknext="block_00042",
},
