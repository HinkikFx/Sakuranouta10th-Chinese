{"text"},
text={
vo={
{"vo",file="fem_had_000039",ch="had"},
},
ja={
{
name={"伯奇"},
"「……っ」",
{"rt2"},
},
},
},
linkback="block_00112",
linknext="block_00114",
},
block_00114={
{"text"},
