linknext="block_00008",
},
block_00008={
{"text"},
text={
vo={
{"vo",file="man_kid_000191",ch="kid"},
},
ja={
{
name={"健一郎"},
"「あの屋敷が",
{"txruby",text="うもれぎのや"},
"埋木舎",
{"txruby"},
"と呼ばれているのはどういう意味だ？」",
{"rt2"},
},
},
},
linkback="block_00007",
linknext="block_00009",
},
block_00009={
{"fg",ch="紗希",size="z2",mode=3,path=":fg/sak/z2/",file="sak_z2d0000",face="d0010",head="sak_z2d",y=-144,lv=5,id=9,zoom=70},
{"text"},
delay={
