{"fg",ch="桜子",size="z1",mode=3,path=":fg/sku/z1/",file="sku_z1b0200",face="b0007",head="sku_z1b",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_skr_000162",ch="skr"},
