ja={
{
name={"直哉"},
"「なんだそりゃ？」",
{"rt2"},
},
},
},
linkback="block_00151",
linknext="block_00153",
},
block_00153={
{"fg",ch="桜子",size="z1",mode=3,path=":fg/sku/z1/",file="sku_z1b0200",face="b0008",head="sku_z1b",lv=4,id=9},
{"text"},
