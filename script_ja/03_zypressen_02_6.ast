linknext="block_00082",
},
block_00082={
{"fg",ch="直哉",size="z1",mode=3,path=":fg/nao/z1/",file="nao_z1i0000",face="i0002",head="nao_z1i",x=345,lv=4,id=9},
{"text"},
text={
ja={
{
name={"直哉"},
