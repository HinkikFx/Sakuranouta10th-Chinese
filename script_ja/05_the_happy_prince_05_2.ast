{"text"},
text={
ja={
{
name={"直哉"},
"「ああ、ここからが本番だ！」",
{"rt2"},
},
},
},
linkback="block_00258",
linknext="block_00260",
},
block_00260={
{"msgoff"},
{"fg",mode=-2,sync=0},
{"colortone",inter=1,ssize=2,ease="in",sfade=3000,style="raster"},
