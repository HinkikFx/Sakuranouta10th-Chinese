{"text"},
text={
vo={
{"vo",file="fem_rna_000074",ch="rna"},
},
ja={
{
name={"里奈"},
"「私だってがんばりますよ」",
{"rt2"},
},
},
},
linkback="block_00218",
linknext="block_00220",
},
block_00220={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1a0210",face="a0005",head="yum_z1a",x=570,lv=4,id=4},
{"text"},
text={
vo={
{"vo",file="fem_yum_000103",ch="yum"},
},
ja={
{
name={"優美"},
"「右に同じく」",
{"rt2"},
},
},
},
