},
block_00029={
{"fg",ch="里奈",size="z1",mode=3,path=":fg/rna/z1/",file="rna_z1b0100",face="b0002",head="rna_z1b",lv=4,id=9},
{"fgact",ch="里奈",act="うなづく",size=5,lv=4,id=9},
{"text"},
text={
ja={
{
"そしてにっこりと笑う。",
{"rt2"},
},
},
},
linkback="block_00028",
linknext="block_00030",
},
block_00030={
{"text"},
text={
ja={
{
