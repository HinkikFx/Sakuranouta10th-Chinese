vo={
{"vo",file="fem_rna_000653",ch="rna"},
},
ja={
{
name={"里奈"},
"「詳しいなぁ。優美」",
{"rt2"},
},
},
},
linkback="block_00037",
linknext="block_00039",
},
block_00039={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1a0100",face="a0011",head="yum_z1a",x=345,lv=4,id=6},
{"fgact",ch="優美",act="びっくり",lv=4,id=6},
{"text"},
