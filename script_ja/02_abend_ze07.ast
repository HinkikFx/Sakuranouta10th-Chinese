},
block_00044={
{"fg",ch="里奈",size="z1",mode=3,path=":fg/rna/z1/",file="rna_z1a0100",face="a0012",head="rna_z1a",x=-345,lv=4,id=5},
{"fgact",ch="里奈",act="びっくり",lv=4,id=5},
{"text"},
delay={
