vo={
{"vo",file="fem_rna_000654",ch="rna"},
},
ja={
{
name={"里奈"},
"「ここまで来る坂道も夢浮坂というよね」",
{"rt2"},
},
},
},
linkback="block_00041",
linknext="block_00043",
},
block_00043={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0110",face="b0012",head="yum_z1b",x=345,lv=4,id=6},
