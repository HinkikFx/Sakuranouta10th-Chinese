},
block_00047={
{"fg",ch="里奈",size="z1",mode=3,path=":fg/rna/z1/",file="rna_z1b0100",face="b0008",head="rna_z1b",x=-345,lv=4,id=5},
{"text"},
text={
vo={
{"vo",file="fem_rna_000656",ch="rna"},
},
ja={
{
name={"里奈"},
"「夢張り？」",
{"rt2"},
},
},
},
linkback="block_00046",
