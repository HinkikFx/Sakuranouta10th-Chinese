ja={
{
name={"直哉"},
"「メガネで人の印象は変わるんだねぇ」",
{"rt2"},
},
},
},
linkback="block_00188",
linknext="block_00190",
},
block_00190={
{"fg",ch="ノノ未",size="z1",ex03="a0097",mode=3,path=":fg/non/z1/",file="non_z1a0200",face="a0017",head="non_z1a",lv=4,id=9},
{"text"},
text={
