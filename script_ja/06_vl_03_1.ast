},
},
},
linkback="block_00202",
linknext="block_00204",
},
block_00204={
{"fg",ch="ノノ未",size="z1",ex03="a0097",mode=3,path=":fg/non/z1/",file="non_z1a0200",face="a0005",head="non_z1a",lv=4,id=9},
