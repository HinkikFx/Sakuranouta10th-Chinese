text={
ja={
{
name={"直哉"},
"「考え事か？」",
{"rt2"},
},
},
},
linkback="block_00060",
linknext="block_00062",
},
block_00062={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1b0200",face="b0017",head="mak_z1b",lv=4,id=9},
