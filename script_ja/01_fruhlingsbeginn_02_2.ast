block_00180={
{"fg",ch="真琴",mode=3,path=":fg/mak/z1/",file="mak_z1a0210",face="a0010",head="mak_z1a",x2=0,x=-345,lv=4,id=9},
{"fg",ch="優美",size="z1",mx=40,mode=3,resize=1,path=":fg/yum/z1/",file="yum_z1a0220",face="a0001",head="yum_z1a",x=345,lv=4,id=6},
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0210",face="a0012",head="mak_z1a",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000358",ch="mkd"},
},
ja={
{
name={"真琴"},
"「あら、川内野さん。どうしたの？」",
{"rt2"},
},
},
},
linkback="block_00179",
linknext="block_00181",
},
block_00181={
{"text"},
text={
ja={
{
name={"直哉"},
"「川内野？　ああ、唯一入部したっていう生意気な一年か」",
{"rt2"},
},
},
},
linkback="block_00180",
linknext="block_00182",
},
block_00182={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1a0200",face="a0016",head="yum_z1a",x=345,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000020",ch="yum"},
},
ja={
{
name={"優美"},
"「ああ、昨日のセクハラ先輩ですね」",
{"rt2"},
},
},
},
linkback="block_00181",
linknext="block_00183",
},
block_00183={
{"mwact",act="びっくり",size=5},
{"text"},
text={
ja={
{
name={"直哉"},
"「セクハラ違うわ！」",
{"rt2"},
},
},
},
linkback="block_00182",
linknext="block_00184",
},
block_00184={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0210",face="a0016",head="mak_z1a",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000359",ch="mkd"},
},
ja={
{
name={"真琴"},
"「やっぱりセクハラしてたの？」",
{"rt2"},
},
},
},
linkback="block_00183",
linknext="block_00185",
},
block_00185={
{"mwact",act="びっくり",size=5},
{"text"},
text={
ja={
{
name={"直哉"},
"「だから違うって！」",
{"rt2"},
},
},
},
linkback="block_00184",
linknext="block_00186",
},
block_00186={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0200",face="a0002",head="mak_z1a",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000360",ch="mkd"},
},
ja={
{
name={"真琴"},
"「そんで？　川内野さん、部活は放課後からだけど」",
{"rt2"},
},
},
},
linkback="block_00185",
linknext="block_00187",
},
block_00187={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1a0220",face="a0003",head="yum_z1a",x=345,lv=4,id=6},
{"fgact",ch="優美",act="うなづく",lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000021",ch="yum"},
},
ja={
{
name={"優美"},
"「それなのですが、入部希望者を連れてきました」",
{"rt2"},
},
},
},
linkback="block_00186",
linknext="block_00188",
},
block_00188={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0200",face="a0024",head="mak_z1a",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000361",ch="mkd"},
},
ja={
{
name={"真琴"},
"「入部希望者？　昼休みに？」",
{"rt2"},
},
},
},
linkback="block_00187",
linknext="block_00189",
},
block_00189={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1a0220",face="a0011",head="yum_z1a",x=345,lv=4,id=6},
{"text"},
delay={
vl01={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1a0200",face="a0008",head="yum_z1a",x=345,lv=4,id=6},
},
},
text={
vo={
{"vo",file="fem_yum_000022",ch="yum"},
},
ja={
{
name={"優美"},
"「はい、放課後はまた大変な騒ぎになりそうなので、今連れてきました。",
{"rt2"},
"あ、もちろん弓張学園美術部に入るのは相当難しい事は、昨日の入部希望者の数を見て分かってます」",
{"rt2"},
},
},
},
linkback="block_00188",
linknext="block_00190",
},
block_00190={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0210",face="a0009",head="mak_z1a",x=-345,lv=4,id=9},
{"fgact",ch="真琴",act="いいえ",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000362",ch="mkd"},
},
ja={
{
name={"真琴"},
"「あ、いや……別に相当難しいわけじゃないんだけどね。あれはたまたまというか……」",
{"rt2"},
},
},
},
linkback="block_00189",
linknext="block_00191",
},
block_00191={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0005",head="yum_z1b",x=345,lv=4,id=6},
{"fgact",ch="優美",act="いいえ",lv=4,id=6},
{"text"},
delay={
vl01={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0200",face="b0016",head="yum_z1b",x=345,lv=4,id=6},
},
},
text={
vo={
{"vo",file="fem_yum_000023",ch="yum"},
},
ja={
{
name={"優美"},
"「いいえ、この美術部には二年生がいない様ですし、それって去年は誰も受からなかったという事ですよね。すごい倍率の中で選ばれた人間だけが弓張学園美術部に入れるのは良く分かります」",
{"rt2"},
},
},
},
linkback="block_00190",
linknext="block_00192",
},
block_00192={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0200",face="a0015",head="mak_z1a",x=-345,lv=4,id=9},
{"fgact",ch="真琴",act="おじぎ",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000363",ch="mkd"},
},
ja={
{
name={"真琴"},
"「そういうわけじゃなくて、特に去年のは……」",
{"rt2"},
},
},
},
linkback="block_00191",
linknext="block_00193",
},
block_00193={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0012",head="yum_z1b",x=345,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000024",ch="yum"},
},
ja={
{
name={"優美"},
"「でも、昨日の人たちはほとんど落とされてましたよね」",
{"rt2"},
},
},
},
linkback="block_00192",
linknext="block_00194",
},
block_00194={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1b0200",face="b0009",head="mak_z1b",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000364",ch="mkd"},
},
ja={
{
name={"真琴"},
"「うん、まぁね。昨日来た新入生の人たちは、なんと言うか……」",
{"rt2"},
},
},
},
linkback="block_00193",
linknext="block_00195",
},
block_00195={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0016",head="yum_z1b",x=345,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000025",ch="yum"},
},
ja={
{
name={"優美"},
"「遺産目当ての人間が多すぎた。そういうわけですよね」",
{"rt2"},
},
},
},
linkback="block_00194",
linknext="block_00196",
},
block_00196={
{"text"},
text={
ja={
{
name={"直哉"},
"「はぁ？」",
{"rt2"},
},
},
},
linkback="block_00195",
linknext="block_00197",
},
block_00197={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1b0200",face="b0013",head="mak_z1b",x=-345,lv=4,id=9},
{"fgact",ch="真琴",act="びっくり",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000365",ch="mkd"},
},
ja={
{
name={"真琴"},
"「い、遺産目当て？」",
{"rt2"},
},
},
},
linkback="block_00196",
linknext="block_00198",
},
block_00198={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0001",head="yum_z1b",x=345,lv=4,id=6},
{"fgact",ch="優美",act="うなづく",lv=4,id=6},
{"text"},
delay={
vl01={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0200",face="b0017",head="yum_z1b",x=345,lv=4,id=6},
},
},
text={
vo={
{"vo",file="fem_yum_000026",ch="yum"},
},
ja={
{
name={"優美"},
"「はい、何でも草薙健一郎の息子であるそこの草薙直哉には莫大な遺産が転がり込んだという噂でしたので」",
{"rt2"},
},
},
},
linkback="block_00197",
linknext="block_00199",
},
block_00199={
{"text"},
text={
ja={
{
"莫大な遺産か。",
{"rt2"},
"まぁ、普通に考えるならば唯一の肉親である俺が健一郎の遺産を受け継いでいるハズだからなぁ。",
{"rt2"},
},
},
},
linkback="block_00198",
linknext="block_00200",
},
block_00200={
{"text"},
text={
ja={
{
name={"直哉"},
"「昨日の女どもは俺の遺産目当てで集まってきたのか……」",
{"rt2"},
},
},
},
linkback="block_00199",
linknext="block_00201",
},
block_00201={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0008",head="yum_z1b",x=345,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000027",ch="yum"},
},
ja={
{
name={"優美"},
"「はい、だから結婚し次第、あなたは用無しなのでたぶん殺されていたと思われます」",
{"rt2"},
},
},
},
linkback="block_00200",
linknext="block_00202",
},
block_00202={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1b0200",face="b0009",head="mak_z1b",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000366",ch="mkd"},
},
ja={
{
name={"真琴"},
"「そうだったんだ……怖い」",
{"rt2"},
},
},
},
linkback="block_00201",
linknext="block_00203",
},
block_00203={
{"text"},
text={
ja={
{
name={"直哉"},
"「なるほ……",
{"rt2"},
"んなわけねーだろ！　だいたい去年もたくさん俺目当てに新入生が来たって話だろ」",
{"rt2"},
},
},
},
linkback="block_00202",
linknext="block_00204",
},
block_00204={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1b0200",face="b0016",head="mak_z1b",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000367",ch="mkd"},
},
ja={
{
name={"真琴"},
"「そうねぇ。でもあの時期から草薙健一郎の体調不良は伝えられてたし……」",
{"rt2"},
},
},
},
linkback="block_00203",
linknext="block_00205",
},
block_00205={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0015",head="yum_z1b",x=345,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000028",ch="yum"},
},
ja={
{
name={"優美"},
"「まぁ、そうでしょう。遺産目当てじゃなければ、こんな男にあれほどの女性が群がるとかあり得ませんから」",
{"rt2"},
},
},
},
linkback="block_00204",
linknext="block_00206",
},
block_00206={
{"text"},
text={
ja={
{
name={"直哉"},
"「そんな事ないだろ。お、俺の魅力で集まったんだろ……あれ」",
{"rt2"},
},
},
},
linkback="block_00205",
linknext="block_00207",
},
block_00207={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0016",head="yum_z1b",x=345,lv=4,id=6},
{"fgact",ch="優美",act="うなづく",lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000029",ch="yum"},
},
ja={
{
name={"優美"},
"「なるほど、美術の腕と目は必ずしも一致しない、それが自らの事になるとまったく客観視出来ない事が良く分かりました」",
{"rt2"},
},
},
},
linkback="block_00206",
linknext="block_00208",
},
block_00208={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1b0200",face="b0004",head="mak_z1b",x=-345,lv=4,id=9},
{"fgact",ch="真琴",act="うんうん",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000368",ch="mkd"},
},
ja={
{
name={"真琴"},
"「あははは、川内野さんおもしろいわね。さすが今年唯一の合格者ね」",
{"rt2"},
},
},
},
linkback="block_00207",
linknext="block_00209",
},
block_00209={
{"text"},
text={
ja={
{
name={"直哉"},
"「どういう基準でお前は人間を選んでるんだよ。",
{"rt2"},
"何だ？　俺に対する敵意が高い順に選んだのか？」",
{"rt2"},
},
},
},
linkback="block_00208",
linknext="block_00210",
},
block_00210={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0200",face="a0016",head="mak_z1a",x=-345,lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000369",ch="mkd"},
},
ja={
{
name={"真琴"},
"「そういうわけじゃないけど、美術をまともにやりたいって子はこの娘ぐらいのものだったのよ」",
{"rt2"},
},
},
},
linkback="block_00209",
linknext="block_00211",
},
block_00211={
{"fg",ch="優美",size="z1",mode=3,path=":fg/yum/z1/",file="yum_z1b0210",face="b0017",head="yum_z1b",x=345,lv=4,id=6},
{"text"},
text={
vo={
{"vo",file="fem_yum_000030",ch="yum"},
},
ja={
{
name={"優美"},
"「他は遺産目当てであなたを殺そうとしてましたから」",
{"rt2"},
},
},
},
linkback="block_00210",
linknext="block_00212",
},
block_00212={
{"quake",mode="mw"},
{"text"},
text={
ja={
{
name={"直哉"},
"「してねぇよ！　みんないい娘だったよ！」",
{"rt2"},
},
},
},
linkback="block_00211",
linknext="block_00213",
},
block_00213={
{"fg",ch="真琴",size="z1",mode=3,path=":fg/mak/z1/",file="mak_z1a0210",face="a0015",head="mak_z1a",x=-345,lv=4,id=9},
{"fgact",ch="真琴",act="おじぎ",lv=4,id=9},
{"text"},
text={
vo={
{"vo",file="fem_mkd_000370",ch="mkd"},
},
ja={
{
name={"真琴"},
"「まともに話もしてないくせによく言うわね」",
{"rt2"},
},
},
},
linkback="block_00212",
linknext="block_00214",
},
block_00214={
{"text"},
text={
ja={
{
name={"直哉"},
"「何言ってるんだよ。短期間でも俺をあれだけちやほやしてくれたんだ。それだけでもあの娘達がいい娘だった事が分かるじゃねぇか」",
{"rt2"},
},
