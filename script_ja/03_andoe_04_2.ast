vo={
{"vo",file="fem_aid_000823",ch="aid"},
},
ja={
{
name={"藍"},
"「……それは健一郎を信頼していたからだよ……」",
{"rt2"},
},
},
},
linkback="block_00091",
linknext="block_00093",
},
block_00093={
{"fg",ch="藍",size="z1",mode=3,path=":fg/aix/z1/",file="aix_z1i0000",face="i0009",head="aix_z1i",x=345,lv=4,id=6},
