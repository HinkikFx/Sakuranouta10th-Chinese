        linkback = "block_00174",
        linknext = "block_00176"
    },
    block_00176 = {
        {"text"},
        text = {
            ja = {
                {
                    name = {"直哉"},
                    "「你以前评价我说像《幸福王子》，而那本书的作者奥斯卡·王尔德，",
